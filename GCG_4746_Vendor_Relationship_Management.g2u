	v.Local.i.declare(long)
	
	F.Intrinsic.Control.If(V.DataTable.dtDuePO.Exists)
		F.Data.DataTable.Close("dtDuePO")
	F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.If(v.Global.bFirstOpen)
	
		F.Data.DataTable.CreateFromSQL("dtDuePO","con",v.Args.Due,True)
		f.Data.DataTable.AddColumn("dtDuePO","CONVERSION","float")
		
'		'ADDED BY WAW TO MULTIPLY THE CONVERSION FACTOR TO ALL QUANTITIES ON THE DASHBOARD
		f.Data.Dictionary.CreateFromSQL("dict","con","Select (PART + LOCATION) AS PARTLOC, FACTOR_CONVERSION as CONVERSION from V_INVENTORY_mstr")
		F.Data.Dictionary.SetDefaultReturn("dict","0")
		''fill from dictionary 
		f.Data.DataTable.FillFromDictionary("DtDuePO","Dict","PARTLOC","CONVERSION")
		f.Data.Dictionary.Close("Dict")
		
		f.Data.Dictionary.CreateFromSQL("dict","con","Select (PURCHASE_ORDER + LEFT(PO_LINE,3)) AS FULLPO, SUM(QUANTITY) as QTY_RECEIVED from V_ITEM_HISTORY where CODE_TRANSACTION IN ('P10', 'P11') GROUP BY FULLPO")
		F.Data.Dictionary.SetDefaultReturn("dict","0")
		''fill from dictionary 
		f.Data.DataTable.FillFromDictionary("DtDuePO","Dict","FULLPO","QTY_RECEIVED")
		f.Data.Dictionary.Close("Dict")				

		f.Data.Dictionary.CreateFromSQL("dict","con","Select (PURCHASE_ORDER + LEFT(PO_LINE,3)) AS FULLPO, (SUM(QTY_RECD_NOT_INSP) - SUM(QTY_RECEIVED)) as QTY_RECD_NOT_INSP from V_PO_HISTORY where FLAG_INSP = 'I' GROUP BY FULLPO")
		F.Data.Dictionary.SetDefaultReturn("dict","0")
		''fill from dictionary 
		f.Data.DataTable.FillFromDictionary("DtDuePO","Dict","FULLPO","QTY_RECD_NOT_INSP")
		f.Data.Dictionary.Close("Dict")				

		f.Intrinsic.Control.For(v.Local.i,0,v.DataTable.DtDuePO.RowCount--,1)	
			f.Intrinsic.Math.sub(v.DataTable.DtDuePO(v.Local.i).QTY_ORDER!fieldvaltrim,v.DataTable.DtDuePO(v.Local.i).QTY_RECEIVED!fieldvaltrim,v.Local.fqty)
			f.Data.DataTable.SetValue("DtDuePO",v.Local.i,"QTY_OPEN",v.Local.fqty)
	
			f.Intrinsic.Math.Mult(v.DataTable.DtDuePO(v.Local.i).QTY_RECD_NOT_INSP!fieldvaltrim,v.DataTable.DtDuePO(v.Local.i).CONVERSION!fieldvaltrim,v.Local.fqty)
			f.Data.DataTable.SetValue("DtDuePO",v.Local.i,"QTY_RECD_NOT_INSP",v.Local.fqty)	
			
		f.Intrinsic.Control.Next(v.Local.i)		

		f.Intrinsic.Control.For(v.Local.i,0,v.DataTable.dtDuePO.RowCount--,1)	
			f.Intrinsic.Math.sub(v.DataTable.dtDuePO(v.Local.i).QTY_ORDER!fieldvaltrim,v.DataTable.dtDuePO(v.Local.i).QTY_RECEIVED!fieldvaltrim,v.Local.fqty)
			f.Data.DataTable.SetValue("dtDuePO",v.Local.i,"QTY_OPEN",v.Local.fqty)
		f.Intrinsic.Control.Next(v.Local.i)
		
		f.Data.DataTable.RemoveColumn("dtDuePO","CONVERSION")	
		f.Data.DataTable.RemoveColumn("dtDuePO","PARTLOC")			
		f.Data.DataTable.RemoveColumn("dtDuePO","FULLPO")	
				
		F.Data.DataTable.AddDisplayPartColumn("dtDuePO",0,"PART","DISPLAY_PART")
		F.Data.DataTable.FillFromDictionary("dtDuePO","dType","TYPE","TYPE1")
		F.Intrinsic.Control.CallSub(loaddvgvduepo)
	F.Intrinsic.Control.EndIf
	
	Gui.FrmVRM.progressBarVRM.Value(50)
	
	F.Intrinsic.Control.Catch
		F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.End 
	F.Intrinsic.Control.EndTry
Program.Sub.loadDuePO.End

