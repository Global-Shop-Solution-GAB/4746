Program.Sub.loadAllPO.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare
	v.Local.i.Declare(long)
	v.Local.fqty.Declare(float)
	v.Local.sexp.Declare(string)
	
	Gui.FrmVRM.progressBarVRM.Value(5)
	
	F.Intrinsic.Control.If(V.DataTable.dtAllPO.Exists)
		F.Data.DataTable.Close("dtAllPO")
	F.Intrinsic.Control.EndIf
	
	F.Intrinsic.Control.If(V.Global.bFirstOpen)
		V.Global.bLoadData.Set(True)

		F.Data.DataTable.CreateFromSQL("dtAllPO","con",v.Args.All,True)
		f.Data.DataTable.AddColumn("dtAllPO","CONVERSION","float")
		
'		'ADDED BY WAW TO MULTIPLY THE CONVERSION FACTOR TO ALL QUANTITIES ON THE DASHBOARD
		f.Data.Dictionary.CreateFromSQL("dict","con","Select (PART + LOCATION) AS PARTLOC, FACTOR_CONVERSION as CONVERSION from V_INVENTORY_mstr")
		F.Data.Dictionary.SetDefaultReturn("dict","0")
		''fill from dictionary 
		f.Data.DataTable.FillFromDictionary("dtaLLpo","Dict","PARTLOC","CONVERSION")
		f.Data.Dictionary.Close("Dict")
		
		f.Data.Dictionary.CreateFromSQL("dict","con","Select (PURCHASE_ORDER + LEFT(PO_LINE,3)) AS FULLPO, SUM(QUANTITY) as QTY_RECEIVED from V_ITEM_HISTORY where CODE_TRANSACTION IN ('P10', 'P11') GROUP BY FULLPO")
		F.Data.Dictionary.SetDefaultReturn("dict","0")
		''fill from dictionary 
		f.Data.DataTable.FillFromDictionary("dtaLLpo","Dict","FULLPO","QTY_RECEIVED")
		f.Data.Dictionary.Close("Dict")				

		f.Data.Dictionary.CreateFromSQL("dict","con","Select (PURCHASE_ORDER + LEFT(PO_LINE,3)) AS FULLPO, (SUM(QTY_RECD_NOT_INSP) - SUM(QTY_RECEIVED)) as QTY_RECD_NOT_INSP from V_PO_HISTORY where FLAG_INSP = 'I' GROUP BY FULLPO")
		F.Data.Dictionary.SetDefaultReturn("dict","0")
		''fill from dictionary 
		f.Data.DataTable.FillFromDictionary("dtaLLpo","Dict","FULLPO","QTY_RECD_NOT_INSP")
		f.Data.Dictionary.Close("Dict")				

		f.Intrinsic.Control.For(v.Local.i,0,v.DataTable.dtAllPO.RowCount--,1)	
			f.Intrinsic.Math.sub(v.DataTable.dtAllPO(v.Local.i).QTY_ORDER!fieldvaltrim,v.DataTable.dtAllPO(v.Local.i).QTY_RECEIVED!fieldvaltrim,v.Local.fqty)
			f.Data.DataTable.SetValue("dtaLLpo",v.Local.i,"QTY_OPEN",v.Local.fqty)
	
			f.Intrinsic.Math.Mult(v.DataTable.dtAllPO(v.Local.i).QTY_RECD_NOT_INSP!fieldvaltrim,v.DataTable.dtAllPO(v.Local.i).CONVERSION!fieldvaltrim,v.Local.fqty)
			f.Data.DataTable.SetValue("dtaLLpo",v.Local.i,"QTY_RECD_NOT_INSP",v.Local.fqty)	
			
